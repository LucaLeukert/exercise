{
    "expo": {
        "name": "Exercise App",
        "slug": "exercise-app",
        "version": "1.0.0",
        "scheme": "expo",
        "platforms": ["ios"],
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/favicon.png"
        },
        "plugins": ["expo-router"],
        "experiments": {
            "typedRoutes": true,
            "tsconfigPaths": true,
            "reactCompiler": true
        },
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "net.leukert.exercise",
            "infoPlist": {
                "ITSAppUsesNonExemptEncryption": false
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.anonymous.expo"
        },
        "extra": {
            "router": {},
            "eas": {
                "projectId": "d871e2ea-e34a-42cf-a738-2d91869f3569"
            }
        },
        "owner": "lucaleu"
    }
}
